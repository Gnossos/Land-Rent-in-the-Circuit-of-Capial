	Shorttitle = {The Local Wreckage of Global Capital},
	Title = {The Local Wreckage of Global Capital: The Subprime Crisis, Federal Policy and High‐Foreclosure Neighborhoods in the {US}},
	Url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1468-2427.2010.00991.x/abstract},
	Urldate = {2012-04-26},
